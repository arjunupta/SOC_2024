#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define f(i,a   ,b) for(ll i=a;i<b;i++)
int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll t;cin >> t;
    while(t--){
        ll n,k;cin >> n >> k;
        if(n==k){
            f(i,0,n) cout << 1 << " ";
            cout << '\n';
        }
        else if(k==1){
            f(i,0,n) cout << i+1 << " ";
            cout << '\n';
        }
        else cout << -1 << '\n';
    }
}
