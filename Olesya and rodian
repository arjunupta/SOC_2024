#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
int main(){
    ll n,t;
    cin >> n >> t;
    if(t==10){
        if(n==1) cout << "-1" << "\n";
        else{
            cout << "1";
            for(ll i=0;i<(n-1);i++) cout << "0";
        }
    }
    else{
        for(ll i=0;i<n;i++) cout << t;
    }
} 
