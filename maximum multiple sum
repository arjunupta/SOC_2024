#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define f(i,a,b) for(long long i=a;i<b;i++)
int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll t,n;cin >>t;
    while(t--){
        cin >> n;
        if(n==3) cout << "3" << "\n";
        else cout << "2" << "\n";
    }
}
