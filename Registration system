#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define f(i,a,b) for(ll i=a;i<b;i++)
int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll n;
    cin >> n;
    string s;
    map<string,ll>mp;
    while(n--){
        cin >> s;
        if(mp[s]==0) cout << "OK" << "\n";
        else cout << s << mp[s] << "\n";
        mp[s]++;
        
    }
}
