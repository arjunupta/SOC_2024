#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define f(i,a,b) for(ll i=a;i<b;i++)
int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll t;cin >> t;
    while(t--){
        ll x1,y1,x2,y2;
        cin >> x1 >> y1 >> x2 >> y2;
        if(x1==min(x1,y1)){
            if(x2>=y2) cout << "NO" << '\n';
            else cout << "YES" << '\n';
        }
        else{
            if(y2>=x2) cout << "NO" << '\n';
            else cout << "YES" << '\n';
        }
    }
}
